{
	"$schema": "https://playground.wordpress.net/blueprint-schema.json",
	"phpExtensionBundles": [
		"kitchen-sink"
	],
	"features": {
		"networking": true
	},
	"constants": {
		"WP_HTTP_BLOCK_EXTERNAL": "true"
	},
	"siteOptions": {
		"permalink_structure": "/%postname%/"
	},
	"steps": [
		{
			"step": "defineWpConfigConsts",
			"consts": {
				"WP_DEBUG": false,
				"WP_DEBUG_DISPLAY": false,
				"DISABLE_WP_CRON": true
			}
		}
	]
}